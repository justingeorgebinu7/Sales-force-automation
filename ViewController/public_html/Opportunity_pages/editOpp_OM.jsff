                  maximumLength="#{bindings.Salesmethod.hints.precision}"
                  shortDesc="#{bindings.Salesmethod.hints.tooltip}" id="it5">
      <f:validator binding="#{bindings.Salesmethod.validator}"/>
    </af:inputText>

<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="navigationMain">
    <default-activity>CustomerAccountParent</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <task-flow-call id="CustomerAccountParent">
      <task-flow-reference>
        <document>/WEB-INF/CustomerAccountParent.xml</document>
        <id>CustomerAccountParent</id>
      </task-flow-reference>
    </task-flow-call>
    <task-flow-call id="AppointmentsAndInteractionsMain">
      <task-flow-reference>
        <document>/WEB-INF/AppointmentsAndInteraction/AppointmentsAndInteractionsMain.xml</document>
        <id>AppointmentsAndInteractionsMain</id>
      </task-flow-reference>
    </task-flow-call>
    <control-flow-rule id="__1">
      <from-activity-id>CustomerAccountParent</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>toAppoinment</from-outcome>
        <to-activity-id>AppointmentsAndInteractionsMain</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
  </task-flow-definition>
</adfc-config>

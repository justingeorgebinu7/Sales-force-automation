<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="notesForContact">
    <default-activity>notesForConTAble</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="notesForConTAble">
      <page>/notesForConTAble.jsff</page>
    </view>
    <view id="editNotesCon">
      <page>/editNotesCon.jsff</page>
    </view>
    <task-flow-call id="addNoteForCon">
      <task-flow-reference>
        <document>/WEB-INF/ContactManagement/addNoteForCon.xml</document>
        <id>addNoteForCon</id>
      </task-flow-reference>
    </task-flow-call>
    <task-flow-call id="deleteNotesForCon">
      <task-flow-reference>
        <document>/WEB-INF/ContactManagement/deleteNotesForCon.xml</document>
        <id>deleteNotesForCon</id>
      </task-flow-reference>
    </task-flow-call>
    <control-flow-rule id="__1">
      <from-activity-id>notesForConTAble</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>toEdit</from-outcome>
        <to-activity-id>editNotesCon</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__6">
        <from-outcome>toAdd</from-outcome>
        <to-activity-id>addNoteForCon</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__7">
        <from-outcome>toDelete</from-outcome>
        <to-activity-id>deleteNotesForCon</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__3">
      <from-activity-id>editNotesCon</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>backFromEdit</from-outcome>
        <to-activity-id>notesForConTAble</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>

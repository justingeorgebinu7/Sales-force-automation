<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="appointmentsForContact">
    <default-activity>AppTableForContacts</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="AppTableForContacts">
      <page>/ContactManagementPages/AppTableForContacts.jsff</page>
    </view>
    <view id="EditAppForContact">
      <page>/ContactManagementPages/EditAppForContact.jsff</page>
    </view>
    <task-flow-call id="deleteAppForContacts1">
      <task-flow-reference>
        <document>/WEB-INF/ContactManagement/deleteAppForContacts.xml</document>
        <id>deleteAppForContacts</id>
      </task-flow-reference>
    </task-flow-call>
    <task-flow-call id="createAppointment">
      <task-flow-reference>
        <document>/WEB-INF/ContactManagement/createAppointment.xml</document>
        <id>createAppointment</id>
      </task-flow-reference>
    </task-flow-call>
    <task-flow-call id="createInviteeInternal_con">
      <task-flow-reference>
        <document>/WEB-INF/ContactManagement/createInviteeInternal_con.xml</document>
        <id>createInviteeInternal_con</id>
      </task-flow-reference>
    </task-flow-call>
    <task-flow-call id="deleteInviteeInt_con">
      <task-flow-reference>
        <document>/WEB-INF/ContactManagement/deleteInviteeInt_con.xml</document>
        <id>deleteInviteeInt_con</id>
      </task-flow-reference>
      <run-as-dialog/>
    </task-flow-call>
    <task-flow-call id="createInviteeExt_con">
      <task-flow-reference>
        <document>/WEB-INF/ContactManagement/createInviteeExt_con.xml</document>
        <id>createInviteeExt_con</id>
      </task-flow-reference>
    </task-flow-call>
    <task-flow-call id="deleteInvExt_con">
      <task-flow-reference>
        <document>/WEB-INF/ContactManagement/deleteInvExt_con.xml</document>
        <id>deleteInvExt_con</id>
      </task-flow-reference>
      <run-as-dialog/>
    </task-flow-call>
    <control-flow-rule id="__1">
      <from-activity-id>AppTableForContacts</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>edit</from-outcome>
        <to-activity-id>EditAppForContact</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__10">
        <from-outcome>create</from-outcome>
        <to-activity-id>createAppointment</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__4">
      <from-activity-id>EditAppForContact</from-activity-id>
      <control-flow-case id="__5">
        <from-outcome>exit</from-outcome>
        <to-activity-id>AppTableForContacts</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__2">
        <from-outcome>toAddInviteeInternal</from-outcome>
        <to-activity-id>createInviteeInternal_con</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__8">
        <from-outcome>deleteInternal</from-outcome>
        <to-activity-id>deleteInviteeInt_con</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__13">
        <from-outcome>toAddExt</from-outcome>
        <to-activity-id>createInviteeExt_con</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__17">
        <from-outcome>toDeleteExt</from-outcome>
        <to-activity-id>deleteInvExt_con</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__6">
      <from-activity-id>AppTableForContacts</from-activity-id>
      <control-flow-case id="__7">
        <from-outcome>deleteAppForContacts1</from-outcome>
        <to-activity-id>deleteAppForContacts1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>

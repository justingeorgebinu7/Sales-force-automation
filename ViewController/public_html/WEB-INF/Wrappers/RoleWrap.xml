<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="RoleWrap">
    <default-activity>router1</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <router id="router1">
      <case id="__8">
        <expression>#{securityContext.userInRole['admin']}</expression>
        <outcome>Y</outcome>
      </case>
      <expression>#{securityContext.userInRole['admin']}</expression>
        <outcome>Y</outcome>
      <default-outcome>N</default-outcome>
    </router>
    <task-flow-return id="taskFlowReturn1">
      <outcome>
        <name>taskFlowReturn1</name>
      </outcome>
    </task-flow-return>
    <task-flow-call id="RoleManagement">
      <task-flow-reference>
        <document>/WEB-INF/RoleManagement/RoleManagement.xml</document>
        <id>RoleManagement</id>
      </task-flow-reference>
    </task-flow-call>
    <method-call id="refresh">
      <method>#{bindings.refresh.execute}</method>
      <outcome>
        <fixed-outcome>refresh</fixed-outcome>
      </outcome>
    </method-call>
    <view id="errorAuth">
      <page>/errorAuth.jsff</page>
    </view>
    <control-flow-rule id="__1">
      <from-activity-id>router1</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>Y</from-outcome>
        <to-activity-id>RoleManagement</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__3">
        <from-outcome>N</from-outcome>
        <to-activity-id>errorAuth</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__4">
      <from-activity-id>errorAuth</from-activity-id>
      <control-flow-case id="__5">
        <from-outcome>refreshPage</from-outcome>
        <to-activity-id>refresh</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__6">
      <from-activity-id>refresh</from-activity-id>
      <control-flow-case id="__7">
        <from-outcome>refresh</from-outcome>
        <to-activity-id>taskFlowReturn1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>

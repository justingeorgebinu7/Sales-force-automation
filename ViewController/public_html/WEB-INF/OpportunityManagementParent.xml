<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="OpportunityManagementParent">
    <default-activity>OpportunityHome</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="OpportunityHome">
      <page>/OpportunityManagement/OpportunityHome.jsf</page>
    </view>
    <view id="OpportunityMenu">
      <page>/OpportunityManagement/OpportunityMenu.jsf</page>
    </view>
    <task-flow-return id="taskFlowReturn1">
      <outcome>
        <name>taskFlowReturn1</name>
      </outcome>
    </task-flow-return>
    <control-flow-rule id="__1">
      <from-activity-id>OpportunityHome</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>toMenu</from-outcome>
        <to-activity-id>OpportunityMenu</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__5">
        <from-outcome>exitTF</from-outcome>
        <to-activity-id>taskFlowReturn1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__3">
      <from-activity-id>OpportunityMenu</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>toHome</from-outcome>
        <to-activity-id>OpportunityHome</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
  </task-flow-definition>
</adfc-config>

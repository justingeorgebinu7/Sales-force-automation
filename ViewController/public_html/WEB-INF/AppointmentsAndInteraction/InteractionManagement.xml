<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="InteractionManagement">
    <default-activity>contactSelection</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="InteractionView">
      <page>/AppointmentsAndInteractions/InteractionView.jsff</page>
    </view>
    <view id="EditInteractions">
      <page>/AppointmentsAndInteractions/EditInteractions.jsff</page>
    </view>
    <view id="SearchInteraction">
      <page>/AppointmentsAndInteractions/SearchInteraction.jsff</page>
    </view>
    <task-flow-call id="createInteractionTF">
      <task-flow-reference>
        <document>/WEB-INF/AppointmentsAndInteraction/createInteractionTF.xml</document>
        <id>createInteractionTF</id>
      </task-flow-reference>
    </task-flow-call>
    <view id="contactSelection">
      <page>/contactSelection.jsff</page>
    </view>
    <control-flow-rule id="__1">
      <from-activity-id>InteractionView</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>editInteraction</from-outcome>
        <to-activity-id>EditInteractions</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__3">
        <from-outcome>searchInteraction</from-outcome>
        <to-activity-id>SearchInteraction</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__8">
        <from-outcome>createInteraction</from-outcome>
        <to-activity-id>createInteractionTF</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__11">
        <from-outcome>backToContack</from-outcome>
        <to-activity-id>contactSelection</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__4">
      <from-activity-id>EditInteractions</from-activity-id>
      <control-flow-case id="__5">
        <from-outcome>backFromEdit</from-outcome>
        <to-activity-id>InteractionView</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__6">
      <from-activity-id>SearchInteraction</from-activity-id>
      <control-flow-case id="__7">
        <from-outcome>backFromSearch</from-outcome>
        <to-activity-id>InteractionView</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__9">
      <from-activity-id>contactSelection</from-activity-id>
      <control-flow-case id="__10">
        <from-outcome>toInteract</from-outcome>
        <to-activity-id>InteractionView</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>

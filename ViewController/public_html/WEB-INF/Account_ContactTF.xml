<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="Account_ContactTF">
    <default-activity>AccountContact</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="AccountContact">
      <page>/CustomerAccountManagement/AccountContact.jsff</page>
    </view>
    <view id="AddContact">
      <page>/CustomerAccountManagement/AddContact.jsff</page>
    </view>
    <view id="EditContact">
      <page>/CustomerAccountManagement/EditContact.jsff</page>
    </view>
    <view id="SearchContact">
      <page>/CustomerAccountManagement/SearchContact.jsff</page>
    </view>
    <control-flow-rule id="__1">
      <from-activity-id>AccountContact</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>goToAddAccount</from-outcome>
        <to-activity-id>AddContact</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__5">
        <from-outcome>goToEdit</from-outcome>
        <to-activity-id>EditContact</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__8">
        <from-outcome>goToSearch</from-outcome>
        <to-activity-id>SearchContact</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__3">
      <from-activity-id>AddContact</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>exitFromAdd</from-outcome>
        <to-activity-id>AccountContact</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__6">
      <from-activity-id>EditContact</from-activity-id>
      <control-flow-case id="__7">
        <from-outcome>exitFromEdit</from-outcome>
        <to-activity-id>AccountContact</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__9">
      <from-activity-id>SearchContact</from-activity-id>
      <control-flow-case id="__10">
        <from-outcome>exitFromSearch</from-outcome>
        <to-activity-id>AccountContact</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>

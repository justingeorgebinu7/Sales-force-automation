<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="CustomerAccountParent">
    <default-activity>CustAccountHome</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="CustAccountHome">
      <page>/CustomerAccountManagement/CustAccountHome.jsf</page>
    </view>
    <task-flow-return id="taskFlowReturn1">
      <outcome>
        <name>taskFlowReturn1</name>
      </outcome>
    </task-flow-return>
    <task-flow-call id="AppointmentsAndInteractionsMain">
      <task-flow-reference>
        <document>/WEB-INF/AppointmentsAndInteraction/AppointmentsAndInteractionsMain.xml</document>
        <id>AppointmentsAndInteractionsMain</id>
      </task-flow-reference>
    </task-flow-call>
    <control-flow-rule id="__1">
      <from-activity-id>CustAccountHome</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>exit</from-outcome>
        <to-activity-id>taskFlowReturn1</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__3">
        <from-outcome>toApp</from-outcome>
        <to-activity-id>AppointmentsAndInteractionsMain</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
  </task-flow-definition>
</adfc-config>

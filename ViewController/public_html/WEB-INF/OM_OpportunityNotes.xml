<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="OM_OpportunityNotes">
    <default-activity>OM_notes</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="OM_notes">
      <page>/OpportunityManagement/OM_notes.jsff</page>
    </view>
    <task-flow-call id="OM_addNoteTF">
      <task-flow-reference>
        <document>/WEB-INF/OM_addNoteTF.xml</document>
        <id>OM_addNoteTF</id>
      </task-flow-reference>
    </task-flow-call>
    <view id="OM_editNotes">
      <page>/OpportunityManagement/OM_editNotes.jsff</page>
    </view>
    <view id="OM_searchNotes">
      <page>/OpportunityManagement/OM_searchNotes.jsff</page>
    </view>
    <control-flow-rule id="__1">
      <from-activity-id>OM_notes</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>addNote</from-outcome>
        <to-activity-id>OM_addNoteTF</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__3">
        <from-outcome>goToEdit</from-outcome>
        <to-activity-id>OM_editNotes</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__6">
        <from-outcome>goToSearch</from-outcome>
        <to-activity-id>OM_searchNotes</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__4">
      <from-activity-id>OM_editNotes</from-activity-id>
      <control-flow-case id="__5">
        <from-outcome>backFromEdit</from-outcome>
        <to-activity-id>OM_notes</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__7">
      <from-activity-id>OM_searchNotes</from-activity-id>
      <control-flow-case id="__8">
        <from-outcome>backFromSearch</from-outcome>
        <to-activity-id>OM_notes</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>

<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="addExtInvitee">
    <default-activity>CreateInsert</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <method-call id="CreateInsert">
      <method>#{bindings.CreateInsert.execute}</method>
      <outcome>
        <fixed-outcome>CreateInsert</fixed-outcome>
      </outcome>
    </method-call>
    <method-call id="Delete">
      <method>#{bindings.Delete.execute}</method>
      <outcome>
        <fixed-outcome>Delete</fixed-outcome>
      </outcome>
    </method-call>
    <view id="OM_AddExtInvitee">
      <page>/Opportunity_pages/OM_AddExtInvitee.jsff</page>
    </view>
    <method-call id="Commit">
      <method>#{bindings.Commit.execute}</method>
      <outcome>
        <fixed-outcome>Commit</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__1">
      <from-activity-id>CreateInsert</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>CreateInsert</from-outcome>
        <to-activity-id>OM_AddExtInvitee</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__3">
      <from-activity-id>OM_AddExtInvitee</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>discard</from-outcome>
        <to-activity-id>Delete</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__5">
        <from-outcome>commit</from-outcome>
        <to-activity-id>Commit</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__6">
      <from-activity-id>Delete</from-activity-id>
      <control-flow-case id="__7">
        <from-outcome>Delete</from-outcome>
        <to-activity-id>Commit</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>

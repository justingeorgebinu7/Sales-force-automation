<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="createUserRole">
    <default-activity>CreateInsert</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__11">
      <managed-bean-name>RoleRange</managed-bean-name>
      <managed-bean-class>Beans.RoleRange</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <method-call id="CreateInsert">
      <method>#{bindings.CreateInsert.execute}</method>
      <outcome>
        <fixed-outcome>CreateInsert</fixed-outcome>
      </outcome>
    </method-call>
    <method-call id="Delete">
      <method>#{bindings.Delete.execute}</method>
      <outcome>
        <fixed-outcome>Delete</fixed-outcome>
      </outcome>
    </method-call>
    <task-flow-return id="taskFlowReturn1">
      <outcome>
        <name>taskFlowReturn1</name>
      </outcome>
    </task-flow-return>
    <view id="createNewUserRole">
      <page>/UserManagementPages/createNewUserRole.jsff</page>
    </view>
    <method-call id="Commit">
      <method>#{bindings.Commit.execute}</method>
      <outcome>
        <fixed-outcome>Commit</fixed-outcome>
      </outcome>
    </method-call>
    <task-flow-call id="invalidUserPopUp">
      <task-flow-reference>
        <document>/WEB-INF/UserManagement/invalidUserPopUp.xml</document>
        <id>invalidUserPopUp</id>
      </task-flow-reference>
      <run-as-dialog>
        <display-type>
          <inline-popup/>
        </display-type>
      </run-as-dialog>
    </task-flow-call>
    <control-flow-rule id="__1">
      <from-activity-id>CreateInsert</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>CreateInsert</from-outcome>
        <to-activity-id>createNewUserRole</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__3">
      <from-activity-id>createNewUserRole</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>discard</from-outcome>
        <to-activity-id>Delete</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__9">
        <from-outcome>save</from-outcome>
        <to-activity-id>Commit</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__10">
        <from-outcome>invalidUserPopUp</from-outcome>
        <to-activity-id>invalidUserPopUp</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__5">
      <from-activity-id>Delete</from-activity-id>
      <control-flow-case id="__6">
        <from-outcome>Delete</from-outcome>
        <to-activity-id>Commit</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__7">
      <from-activity-id>Commit</from-activity-id>
      <control-flow-case id="__8">
        <from-outcome>Commit</from-outcome>
        <to-activity-id>taskFlowReturn1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
